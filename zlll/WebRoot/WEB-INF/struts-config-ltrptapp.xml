<?xml version="1.0" encoding="gb2312" ?>
<!DOCTYPE struts-config PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
"http://struts.apache.org/dtds/struts-config_1_3.dtd">
<struts-config>
	<action-mappings>
		<action path="/ltrptapp/manage/reportedit"
			type="gov.mof.fasp.ifmis.ltrptapp.manage.LTReportEditAction"
			parameter="listReportEditAction">
			<forward name="success" path='/ltrptapp/manage/editor' />
		</action>
		<action path="/ltrptapp/manage/reportlist"
			type="gov.mof.fasp.ifmis.ltrptapp.manage.LTReportEditAction"
			parameter="listReportQueryAction">
			<forward name="success" path='/ltrptapp/manage/query' />
		</action>
		<action path="/ltrptapp/manage/add_reportedit"
			type="gov.mof.fasp.ifmis.ltrptapp.manage.LTReportEditAction"
			parameter="turnToSaveReportEditAction">
			<forward name="success" path='/ltrptapp/manage/add_reportedit' />
		</action>
		<action path="/ltrptapp/manage/reportedit_add"
			type="gov.mof.fasp.ifmis.ltrptapp.manage.LTReportEditAction"
			parameter="saveReportEditAction">
		</action>
		<action path="/ltrptapp/manage/update_reportedit"
			type="gov.mof.fasp.ifmis.ltrptapp.manage.LTReportEditAction"
			parameter="turnToUptReportEditAction">
			<forward name="success" path='/ltrptapp/manage/change_reportedit' />
		</action>
		<action path="/ltrptapp/manage/reportedit_update"
			type="gov.mof.fasp.ifmis.ltrptapp.manage.LTReportEditAction"
			parameter="uptReportEditAction">
			<forward name="success" path='/ltrptapp/manage/reportedit.do' />
		</action>
		<action path="/ltrptapp/manage/reportedit_del"
			type="gov.mof.fasp.ifmis.ltrptapp.manage.LTReportEditAction"
			parameter="delReportEditAction">
			<forward name="success" path='/ltrptapp/manage/reportedit.do' />
		</action>
		<action path="/ltrptapp/manage/list_reportedit_find"
			type="gov.mof.fasp.ifmis.ltrptapp.manage.LTReportEditAction"
			parameter="listReportEditAction">
			<forward name="success" path='/ltrptapp/manage/reportedit.do' />
		</action>
	</action-mappings>
</struts-config>
